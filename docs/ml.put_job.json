            },
            "expand_wildcards": {
                "type": "enum",
                "options": [
                    "open",
                    "closed",
                    "hidden",
                    "none",
                    "all"
                ],
                "description": "Whether source index expressions should get expanded to open or closed indices (default: open). Only set if datafeed_config is provided."
            }
